{
  "name": "@agodin3z/nest-typeorm-template",
  "version": "0.1.0",
  "description": "TypeORM + NestJS API template",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "^16.19.x",
    "yarn": "^1.22.x"
  },
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "pm2": "pm2 start ./dist/main.js --name typeorm-api",
    "pm2:restart": "pm2 reload typeorm-api",
    "pm2:stop": "pm2 stop typeorm-api",
    "pm2:logs": "pm2 logs typeorm-api",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --color --max-warnings=0",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:ts": "tsc --project tsconfig.json --noEmit",
    "test": "npx dotenv -e .env.test -- jest --runInBand --detectOpenHandles",
    "test:watch": "npx dotenv -e .env.test -- jest --watch",
    "test:cov": "npx dotenv -e .env.test -- jest --coverage --runInBand",
    "test:debug": "npx dotenv -e .env.test -- node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npx dotenv -e .env.test -- jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:drop": "npm run typeorm -- schema:drop -d ./src/database/typeorm.config.ts",
    "typeorm:drop:test": "npx dotenv -e .env.test -- npm run typeorm -- schema:drop -d ./src/database/typeorm.config.ts",
    "migrations": "npm run typeorm -- migration:run -d ./src/database/typeorm.config.ts",
    "migrations:create": "f() { npm run typeorm -- migration:create ./src/database/migrations/\"${1:-new-migration}\"; }; f",
    "migrations:revert": "npm run typeorm -- migration:revert -d ./src/database/typeorm.config.ts",
    "migrations:generate": "f() { npm run typeorm -- migration:generate -d ./src/database/typeorm.config.ts ./src/database/migrations/\"${1:-generated}\"; }; f",
    "migrations:test": "npx dotenv -e .env.test -- npm run typeorm -- migration:run -d ./src/database/typeorm.config.ts",
    "migrations:test:revert": "npx dotenv -e .env.test -- npm run typeorm -- migration:revert -d ./src/database/typeorm.config.ts",
    "seeds": "ts-node ./node_modules/@jorgebodega/typeorm-seeding/dist/cli.js seed -d ./src/database/typeorm.config.ts ./src/database/seeds/*.seed.ts",
    "seeds:test": "npx dotenv -e .env.test -- npm run seeds"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/terminus": "^9.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@sendgrid/mail": "^7.7.0",
    "axios": "^1.7.4",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.2.1",
    "chokidar": "^3.5.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-timeout": "^1.9.0",
    "crypto-js": "^4.2.0",
    "ejs": "^3.1.10",
    "helmet": "^6.0.1",
    "html-pdf": "^3.0.1",
    "ioredis": "^5.3.1",
    "joi": "^17.8.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "ms": "^2.1.3",
    "nanoid": "^5.0.9",
    "passport": "^0.6.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.10.0",
    "pluralize": "^8.0.0",
    "qr-with-logo": "^1.0.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.0",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@jorgebodega/typeorm-seeding": "^6.0.1",
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.1",
    "@types/node": "^18.15.2",
    "@types/open-graph": "^0.2.2",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.14",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "lint-staged": "^13.2.0",
    "pm2": "^5.3.0",
    "prettier": "^2.8.4",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^8.0.18",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.7.4",
    "utility-types": "^3.10.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.controller.ts",
      "**/*.service.ts",
      "**/*.resolver.ts",
      "**/*.factory.ts",
      "**/utils/*.ts",
      "**/events/*.ts",
      "**/*.validator.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "jest-extended"
    ]
  },
  "resolutions": {
    "sharp": "^0.29.0"
  },
  "lint-staged": {
    "{src,apps,libs,test}/**/*.ts": [
      "eslint --color --max-warnings=0",
      "prettier --write"
    ]
  },
  "volta": {
    "node": "16.19.1",
    "yarn": "1.22.19"
  }
}
